{
	"name": "jjjj-cli",
	"version": "1.0.7",
	"main": "index.js",
	"type": "module",
	"bin": {
		"j": "./bin/run.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "npx tsc --pretty",
		"prepublishOnly": "npm run build",
		"dev:install": "node install.js",
		"dev:uninstall": "rm ~/.local/bin/j",
